---
import Study from "./icons/Study.astro";

import TimeLine from "./ui/TimeLine/TimeLine.astro";
import { mockEducation } from "../data/education.mock";
---

<section
  class="bg-secondary rounded-2xl p-8 flex flex-col gap-6 animate-pulse-fade-in animate-duration-500 animate-delay-400"
>
  <div class="flex gap-4 items-center">
    <Study />
    <h2>Education</h2>
  </div>

  <TimeLine data={mockEducation} />
</section>
